local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/KanekiCow/gamehubvaliant/main/ui%20lib"))();


local Gamer = library:CreateSection("YBA");
Gamer:Button("~ Press Right Shift To Hide GUI ~");
function LoadMe(link)
    loadstring(game:HttpGet(link))()
end

Gamer:Button("Pilot Any Stand!", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/lmmake/jojo-/main/Yba%20pilot"))()
end)
Gamer:Button("Auto Farm GUI", function()
 loadstring(game:HttpGet(("https://aizen.ml/uploads/yba.txt")))()
end)
Gamer:Button("Xenon GUI", function()
 loadstring(game:HttpGet('https://raw.githubusercontent.com/Tobias020108Back/YBA-AUT/main/Xenon-X-ATJS'))() 
end)
Gamer:Button("NexHub(need key)", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/CopyReal/NexHub/main/NexHubLoader"))()
end)
library:Ready();
